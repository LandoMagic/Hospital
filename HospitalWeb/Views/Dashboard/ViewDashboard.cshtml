
@{
    ViewBag.Title = "ViewDashboard";
    Layout = "~/Views/Shared/TemplateLayout/_AppTemplateMasterLayout.cshtml";
}


<link href="~/Content/css/datepicker3.css" rel="stylesheet">
<link href="~/Content/css/styles.css" rel="stylesheet">

<!--Icons-->
<script src="~/Scripts/js/lumino.glyphs.js"></script>

<!--[if lt IE 9]>
<script src="js/html5shiv.js"></script>
<script src="js/respond.min.js"></script>
<![endif]-->




<div class="row">
    <div class="col-md-6">
        <div class="panel panel-default">
            <div class="panel-heading">Deaths Chart (Last 7 Days)</div>
            <div class="panel-body">
                <div class="canvas-wrapper">
                    <canvas class="chart" id="pie-chart"></canvas>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="panel panel-default">
            <div class="panel-heading">Births Chart (Last 7 Days)</div>
            <div class="panel-body">
                <div class="canvas-wrapper">
                    <canvas class="chart" id="doughnut-chart"></canvas>
                </div>
            </div>
        </div>
    </div>
</div><!--/.row-->

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">Bar Chart</div>
            <div class="panel-body">
                <div class="canvas-wrapper">
                    <canvas class="main-chart" id="bar-chart" height="200" width="600"></canvas>
                </div>
            </div>
        </div>
    </div>
</div><!--/.row-->

<div class="row">
    <div class="col-xs-6 col-md-3">
        <div class="panel panel-default">
            <div class="panel-body easypiechart-panel">
                <h4>Total Birth</h4>
                <div class="easypiechart" id="easypiechart-blue" data-percent="92">
                    <span class="percent">92%</span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-6 col-md-3">
        <div class="panel panel-default">
            <div class="panel-body easypiechart-panel">
                <h4>Label:</h4>
                <div class="easypiechart" id="easypiechart-orange" data-percent="65">
                    <span class="percent">65%</span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-6 col-md-3">
        <div class="panel panel-default">
            <div class="panel-body easypiechart-panel">
                <h4>Label:</h4>
                <div class="easypiechart" id="easypiechart-teal" data-percent="56">
                    <span class="percent">56%</span>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-6 col-md-3">
        <div class="panel panel-default">
            <div class="panel-body easypiechart-panel">
                <h4>Label:</h4>
                <div class="easypiechart" id="easypiechart-red" data-percent="27">
                    <span class="percent">27%</span>
                </div>
            </div>
        </div>
    </div>
</div><!--/.row-->

@section scripts {
 <script src="~/Scripts/js/bootstrap-datepicker.js"></script>
<script src="~/Scripts/js/chart.min.js"></script>
<script src="~/Scripts/js/chart-data.js"></script>
<script src="~/Scripts/js/easypiechart.js"></script>
<script src="~/Scripts/js/easypiechart-data.js"></script>

<script>
	
</script>	
}
